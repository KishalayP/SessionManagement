<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>sourceServlet</servlet-name>
    <servlet-class>main.java.SourceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sourceServlet</servlet-name>
    <url-pattern>/sourceServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>targetServlet</servlet-name>
    <servlet-class>main.java.TargetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>targetServlet</servlet-name>
    <url-pattern>/targetServlet</url-pattern>
  </servlet-mapping>
</web-app>
